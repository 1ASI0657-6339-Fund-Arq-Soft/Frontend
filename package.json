{
  "name": "age-care",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "gen:openapi:residents": "openapi-typescript http://localhost:8081/v3/api-docs --output src/app/core/models/generated/residents.types.ts",
    "gen:openapi:appointments": "openapi-typescript http://localhost:8085/v3/api-docs --output src/app/core/models/generated/appointments.types.ts",
    "gen:openapi:users": "openapi-typescript http://localhost:8083/v3/api-docs --output src/app/core/models/generated/users.types.ts",
    "gen:openapi:notifications": "openapi-typescript http://localhost:8084/v3/api-docs --output src/app/core/models/generated/notifications.types.ts",
    "gen:openapi:payments": "openapi-typescript http://localhost:8082/v3/api-docs --output src/app/core/models/generated/payments.types.ts",
    "gen:openapi:iam": "openapi-typescript http://localhost:8080/v3/api-docs --output src/app/core/models/generated/iam.types.ts",
    "gen:openapi:all": "npm run gen:openapi:residents && npm run gen:openapi:appointments && npm run gen:openapi:users && npm run gen:openapi:notifications && npm run gen:openapi:payments && npm run gen:openapi:iam"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.0",
    "@angular/compiler": "^20.3.0",
    "@angular/core": "^20.3.0",
    "@angular/forms": "^20.3.0",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@stripe/stripe-js": "^8.5.3",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^13.0.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^20.3.8",
    "@angular/cli": "^20.3.8",
    "@angular/compiler-cli": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "@types/stripe-v3": "^3.1.33",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
    ,
    "openapi-typescript": "^12.2.0"
  }
}
